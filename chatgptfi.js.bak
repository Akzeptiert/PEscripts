function getRandomElement(array) {
  return array[Math.floor(Math.random() * array.length)];
}


function generateSentence(userRequest) {
  if (!userRequest) return "Запрос пустой.";

  const words = userRequest;
  const sentence = [];

  sentence.push(words);

  
  const descriptionCount = Math.floor(1 + Math.random() * 2); 
  const phraseCategories = Object.keys(phrases);

  for (let i = 0; i < descriptionCount; i++) {
    const randomCategory = getRandomElement(phraseCategories);
    sentence.push(getRandomElement(phrases[randomCategory]));
  }

  
  const endingCategory = getRandomElement(Object.keys(sentenceEndings));
  sentence.push(getRandomElement(sentenceEndings[endingCategory]));

  
  return sentence.join(" ");
}

function highlightKeyPoints(text) {
  
  const sentences = text.split(/\.|\?|\!/);

  
  const cleanSentences = sentences.map(sentence => sentence.trim());

  
  const wordFrequencies = {};
  cleanSentences.forEach(sentence => {
    const words = sentence.split(/\s+/);
    words.forEach(word => {
      if (wordFrequencies[word]) {
        wordFrequencies[word]++;
      } else {
        wordFrequencies[word] = 1;
      }
    });
  });

  
  const keyWords = Object.entries(wordFrequencies)
    .sort((a, b) => b[1] - a[1])
    .slice(0, 5) 
    .map(entry => entry[0]);

  
  const keySentences = cleanSentences.filter(sentence => {
    return keyWords.some(word => sentence.includes(word));
  });

  
  return keySentences.join(". "); 
}

