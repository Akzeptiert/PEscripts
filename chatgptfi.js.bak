function getRandomIdea(category) {
  const ideas = ideaBank[category];
  const randomIndex = Math.floor(Math.random() * ideas.length);
  return ideas[randomIndex];
}

function toBase64(input) {
    const chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
    let str = "", i = 0;

    while (i < input.length) {
        let a = input.charCodeAt(i++);
        let b = input.charCodeAt(i++);
        let c = input.charCodeAt(i++);

        let enc1 = a >> 2;
        let enc2 = ((a & 3) << 4) | (b >> 4);
        let enc3 = ((b & 15) << 2) | (c >> 6);
        let enc4 = c & 63;

        if (isNaN(b)) {
            enc3 = enc4 = 64;
        } else if (isNaN(c)) {
            enc4 = 64;
        }

        str += chars.charAt(enc1) + chars.charAt(enc2) + chars.charAt(enc3) + chars.charAt(enc4);
    }
    return str;
}

function fromBase64(encoded) {
    const chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
    let str = "", i = 0;

    while (i < encoded.length) {
        let enc1 = chars.indexOf(encoded.charAt(i++));
        let enc2 = chars.indexOf(encoded.charAt(i++));
        let enc3 = chars.indexOf(encoded.charAt(i++));
        let enc4 = chars.indexOf(encoded.charAt(i++));

        let a = (enc1 << 2) | (enc2 >> 4);
        let b = ((enc2 & 15) << 4) | (enc3 >> 2);
        let c = ((enc3 & 3) << 6) | enc4;

        str += String.fromCharCode(a);
        if (enc3 !== 64) str += String.fromCharCode(b);
        if (enc4 !== 64) str += String.fromCharCode(c);
    }
    return str;
}

function encryptText(input) {
    return input.split('').map(char => char.charCodeAt(0)).join(',');
}

function decryptText(encrypted) {
    return encrypted.split(',').map(num => String.fromCharCode(Number(num))).join('');
}

